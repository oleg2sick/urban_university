def print_params(a = 1, b = 'строка', c = True):        #создал функицию с тремя параметрами из условия задачи
    print(a, b, c)                                      #вывод этих параметров

print_params()
print_params(b = 25)                                    #проверил, работают ли вызовы с заданными в вызове функции параметрами
print_params(c = [1, 2, 3])

values_list = [2, 'text', 2.5]                          #создал список с тремя элементами из условий задачи
values_dict = {'a': 3, 'b': 'string', 'c': False}       #словарь с тремя элементами из условий задачи

print_params(*values_list)                              #вызвал функцию с созданным список
print_params(**values_dict)                             #вызвал функцию с созданным словарем

values_list_2 = [54.32, 'Строка']
print_params(*values_list_2, 42)                        #вызвал функцию по условию задачи
