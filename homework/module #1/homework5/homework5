immutable_var = 1, 2, 3, 'test', False, [4, 5, 6]       #создал кортеж

print(immutable_var)                                    #вывел кортеж на экран

#immutable_var[0] = 8                                   #попытался изменить значение в кортеже

immutable_var[5].append(7)                              #значения изменяемых элементов в кортеже можно изменить, например значение в списке
print(immutable_var)

mutable_list = ['fake', 'sigma']                        #создал список

mutable_list[0] = True                                  #изменил первый элемент списка

print(mutable_list)                                     #вывел измененный список на экран
